# =============================================================================
# MedAgentBench Environment Configuration
# =============================================================================
# Copy this file to .env and fill in your API keys

# Required: OpenAI API key for the purple agent (LLM)
OPENAI_API_KEY=your_openai_api_key_here

# Optional: Alternative LLM model (default: openai/gpt-4o)
# Supports any model available via litellm
# MEDAGENT_LLM_MODEL=openai/gpt-4o

# =============================================================================
# External Services (must be running for evaluation)
# =============================================================================

# FHIR Server URL (default: localhost:8080)
# Start with: docker run -p 8080:8080 jyxsu6/medagentbench:latest
MCP_FHIR_API_BASE=http://localhost:8080/fhir/

# MCP Server URL (default: localhost:8002)
# Start with: uv run python -m src.mcp.server
MCP_SERVER_URL=http://localhost:8002

# =============================================================================
# Task Configuration
# =============================================================================

# Path to tasks JSON file
MEDAGENTBENCH_TASKS_FILE=src/mcp/resources/tasks/tasks.json

# =============================================================================
# Output Configuration (for original launcher)
# =============================================================================

# Output directory for evaluation results
MEDAGENT_OUTPUT_DIR=outputs/medagentbench

